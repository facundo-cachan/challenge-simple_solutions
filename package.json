{
  "name": "simple_solutions",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "clean:ios": "cd ios/ && rm -rf build/ Pods/ Podfile.lock && cd ..",
    "clean:android": "cd android && ./gradlew clean && cd ..",
    "update:android": "cd android/ && ./gradlew :app:dependencies && cd ..",
    "update:ios": "cd ios/ && NO_FLIPPER=1 pod install --verbose && cd ..",
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "lint": "eslint .",
    "tsc": "tsc --noEmit",
    "server": "json-server --watch db.json --port 3001",
    "start": "react-native start --reset-cache",
    "test": "jest --colors --detectOpenHandles",
    "test:coverage": "jest --colors --coverage",
    "test:watch": "jest --colors --detectOpenHandles --watchAll"
  },
  "dependencies": {
    "@react-navigation/native": "6.1.8",
    "@react-navigation/native-stack": "6.9.14",
    "@react-navigation/stack": "6.3.18",
    "axios": "1.6.0",
    "buffer": "6.0.3",
    "react": "18.2.0",
    "react-native": "0.72.5",
    "react-native-vector-icons": "10.0.0",
    "recoil": "0.7.7"
  },
  "devDependencies": {
    "@babel/core": "7.23.0",
    "@babel/plugin-proposal-nullish-coalescing-operator": "7.18.6",
    "@babel/plugin-proposal-optional-chaining": "7.21.0",
    "@babel/plugin-proposal-private-methods": "7.18.6",
    "@babel/plugin-transform-arrow-functions": "7.22.5",
    "@babel/plugin-transform-flow-strip-types": "7.22.5",
    "@babel/plugin-transform-shorthand-properties": "7.22.5",
    "@babel/plugin-transform-template-literals": "7.22.5",
    "@babel/preset-env": "7.22.20",
    "@babel/runtime": "7.23.1",
    "@react-native-community/eslint-config": "3.2.0",
    "@react-native-community/slider": "4.4.3",
    "@react-native/eslint-config": "0.73.1",
    "@react-native/metro-config": "0.73.1",
    "@testing-library/react-native": "12.3.0",
    "@tsconfig/react-native": "3.0.2",
    "@types/react": "18.2.23",
    "@types/react-native-vector-icons": "6.4.15",
    "@types/react-test-renderer": "18.0.3",
    "@typescript-eslint/eslint-plugin": "6.7.3",
    "@typescript-eslint/parser": "6.7.3",
    "babel-loader": "9.1.3",
    "babel-plugin-module-resolver": "5.0.0",
    "eslint": "8.50.0",
    "eslint-config-prettier": "9.0.0",
    "eslint-config-standard-with-typescript": "39.1.0",
    "eslint-plugin-import": "2.28.1",
    "eslint-plugin-prettier": "5.0.0",
    "eslint-plugin-promise": "6.1.1",
    "eslint-plugin-react": "7.33.2",
    "eslint-plugin-react-hooks": "4.6.0",
    "eslint-plugin-react-native": "4.1.0",
    "eslint-plugin-sort-keys-fix": "1.1.2",
    "json-server": "0.17.3",
    "metro-react-native-babel-preset": "0.77.0",
    "prettier": "3.0.3",
    "react-dom": "18.2.0",
    "react-native-dotenv": "3.4.9",
    "react-native-gesture-handler": "2.13.1",
    "react-native-gradle-plugin": "0.71.19",
    "react-native-reanimated": "3.5.4",
